  "dependencies": {
    "body-parser": "1.8.1",
    "chain-async": "1.0.1",
    "cookie-parser": "1.3.3",
    "es6-promise": "4.0.3",
    "express": "^4.15.3",
    "lodash": "4.11.1",
    "log4js": "0.6.35",
    "merge": "1.2.0",
    "mongoose": "4.4.13",
    "mongoose-auto-increment": "5.0.1",
    "morgan": "1.3.0",
    "mpromise": "0.5.5",
    "oauth2-server": "2.4.1",
    "object-path": "0.9.2",
    "properties-reader": "0.0.15",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "chai": "3.2.0",
    "chai-http": "1.0.0",
    "chai-sorted": "0.1.0",
    "chai-things": "0.2.0",
    "eslint": "3.7.1",
    "gulp": "3.9.1",
    "gulp-markdown-docs": "0.1.5",
    "gulp-mocha": "2.2.0",
    "gulp-spawn-mocha": "2.2.2",
    "mocha": "2.4.5",
    "nodemon": "1.11.0"
  },
