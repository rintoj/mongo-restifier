{
  "name": "mongo-restifier",
  "version": "2.3.5",
  "description": "A restful apification library for MongoDB with build-in oAuth2 based authentication and authorization",
  "main": "src/index.js",
  "scripts": {
    "start": "node demo/api.js",
    "develop": "nodemon --watch src demo/api.js",
    "test": "./node_modules/mocha/bin/mocha",
    "debug:test": "./node_modules/mocha/bin/mocha --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rintoj/mongo-restifier.git"
  },
  "keywords": [
    "mongoDB",
    "api",
    "restful api",
    "oAuth2"
